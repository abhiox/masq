{
  "name": "new-app-2",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "main": "./src/electron/main.js",
  "dependencies": {
    "concurrently": "^5.3.0",
    "electron": "^9.1.1",
    "electron-json-storage": "^4.2.0",
    "moment": "^2.27.0",
    "react": "^16.13.1",
    "react-data-table-component": "^6.11.0",
    "react-datepicker": "^3.1.3",
    "react-dom": "^16.13.1",
    "react-export-excel": "^0.5.3",
    "react-router-dom": "^5.2.0",
    "react-toast-notifications": "^2.4.0",
    "styled-components": "^5.1.1",
    "wait-on": "^5.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "rm -rf build && react-scripts build",
    "eject": "react-scripts eject",
    "develop": "npm run build && electron .",
    "ebuild": "npm run build && node_modules/.bin/build",
    "dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "package-win": "electron-packager . masq --overwrite --asar=true --platform=win32 --arch=x64 --icon=icon.ico --prune=true --out=release-builds --version-string.CompanyName=NA --version-string.FileDescription=CE --version-string.ProductName=\"MASQ\""
  },
  "devDependencies": {
    "electron-packager": "^14.2.1",
    "react-scripts": "^3.4.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
